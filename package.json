{
  "name": "toolsjs",
  "version": "1.0.0",
  "description": "common tools for js",
  "main": "mian.js",
  "type": "module",
  "scripts": {
    "dev": "rollup -c ./rollup.config.mjs -w",
    "build": "rollup -c ./rollup.config.mjs",
    "prepublish": "yarn version && yarn build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/killonepersonintensteps/toolsJs.git"
  },
  "keywords": [
    "tools",
    "Javascript",
    "Typescript"
  ],
  "author": "lixiaoning",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tenstepskilloneperson/toolsJs/issues"
  },
  "homepage": "https://github.com/tenstepskilloneperson/toolsJs#readme",
  "dependencies": {
    "chalk": "^5.0.1",
    "rollup": "^2.77.2",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-typescript": "^8.4.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.23.0",
    "husky": "3.1.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rollup-plugin-serve": "^2.0.0",
    "typescript": "^4.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,js}": [
      "node ./node_modules/.bin/prettier",
      "node ./node_modules/.bin/eslint --fix --color"
    ]
  }
}
